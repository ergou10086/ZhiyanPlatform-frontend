{
  "name": "zhiyan-front",
  "version": "1.0.0",
  "description": "智研前端项目",
  "main": "index.js",
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "dev": "vue-cli-service serve",
    "docs:dev": "cd docs && npm run docs:dev",
    "docs:build": "cd docs && vuepress build docs",
    "start": "npm-run-all --parallel serve docs:dev",
    "build:all": "npm run docs:build && npm run build && npm run docs:deploy",
    "docs:deploy": "mkdir -p dist/docs && cp -r docs/.vuepress/dist/* dist/docs/"
  },
  "dependencies": {
    "axios": "^0.27.2",
    "chart.js": "^2.9.4",
    "element-ui": "^2.15.14",
    "highlight.js": "^11.11.1",
    "json-bigint": "^1.0.0",
    "marked": "^11.2.0",
    "three": "^0.181.2",
    "vue": "^2.6.14",
    "vue-chartjs": "^3.5.1",
    "vue-router": "^3.5.4",
    "vue-select": "^3.20.4"
  },
  "devDependencies": {
    "@vue/cli-service": "^5.0.8",
    "npm-run-all": "^4.1.5",
    "vue-template-compiler": "^2.6.14"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead"
  ]
}
